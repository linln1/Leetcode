class Solution {
public:
    bool isOneBitCharacter(vector<int>& bits) {
        int size = bits.size();
        int i=0;
        while(i<size)
        {
            if(bits[i]==1)
                i+=2;
            else if(i!=size-1&&bits[i]==0)
                i+=1;
            else if(i==size-1&&bits[i]==0)
                return true;
        }
        return false;
    }
};//   C++
